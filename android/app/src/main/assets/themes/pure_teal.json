{
  "name": "Pure Teal",
  "author": "Neoscribe",
  "type": "light",
  "colors": {
    "global.colorPrimary": "#00796B",
    "global.colorOutline": "#E0E0E0",
    "global.colorBackgroundPrimary": "#FDFDFD",
    "global.colorBackgroundSecondary": "#F5F5F5",
    "global.colorBackgroundTertiary": "#F5F5F5",
    "global.colorTextAndIconPrimary": "#212121",
    "global.colorTextAndIconPrimaryInverse": "#FFFFFF",
    "global.colorTextAndIconSecondary": "#757575",
    "global.colorTextAndIconDisabled": "#BDBDBD",
    "global.colorTextAndIconAdditional": "#FFD700",
    "global.colorTextAndIconSuccess": "#2E7D32",
    "global.colorTextAndIconError": "#AD1457",
    "editor.background": "#FDFDFD",
    "editor.foreground": "#212121",
    "editor.whitespace": "#E0E0E0",
    "editor.cursor": "#00796B",
    "editor.handle": "#00796B",
    "editor.selection.background": "#B2DFDB",
    "editor.currentLine.background": "#F5F5F5",
    "editor.highlightedDelimiters.background": "#E0F2F1",
    "editor.highlightedDelimiters.foreground": "#00796B",
    "editor.matchHighlight.background": "#C8E6C9",
    "editor.lineNumber.divider": "#E0E0E0",
    "editor.lineNumber.background": "#FDFDFD",
    "editor.lineNumber.foreground": "#9E9E9E",
    "editor.lineNumber.activeForeground": "#00796B",
    "editor.indentGuide.background": "#E0E0E0",
    "editor.indentGuide.activeBackground": "#00796B",
    "editor.popupWindow.corner": "#E0E0E0",
    "editor.popupWindow.background": "#FFFFFF",
    "editor.popupWindow.activeBackground": "#F5F5F5"
  },
  "tokenColors": [
    {
      "settings": {
        "background": "#FDFDFD",
        "foreground": "#212121"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#9E9E9E"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#00897B"
      }
    },
    {
      "name": "Strings",
      "scope": "string",
      "settings": {
        "foreground": "#2E7D32"
      }
    },
    {
      "name": "String Interpolation",
      "scope": "punctuation.definition.template-expression",
      "settings": {
        "foreground": "#00796B"
      }
    },
    {
      "name": "String Interpolation Body",
      "scope": "expression.string,meta.template.expression",
      "settings": {
        "foreground": "#212121"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#AD1457"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language,variable.language,constant.character.escape",
      "settings": {
        "foreground": "#00796B"
      }
    },
    {
      "name": "User-defined constant",
      "scope": "constant.character,constant.other",
      "settings": {
        "foreground": "#AD1457"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword,keyword.operator.new,keyword.operator.delete,keyword.operator.expression",
      "settings": {
        "foreground": "#00796B"
      }
    },
    {
      "name": "Types, Class Types",
      "scope": "storage.type.generic,meta.return.type,meta.type.annotation,meta.type.parameters,support.type.primitive",
      "settings": {
        "foreground": "#1565C0"
      }
    },
    {
      "name": "Storage Type",
      "scope": "storage,storage.type,storage.modifier",
      "settings": {
        "foreground": "#00796B"
      }
    },
    {
      "name": "Class constructor",
      "scope": "new.expr entity.name.type",
      "settings": {
        "foreground": "#00796B"
      }
    },
    {
      "name": "Function",
      "scope": "support.function,entity.name.function",
      "settings": {
        "foreground": "#1565C0"
      }
    },
    {
      "name": "Function Argument",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#00695C"
      }
    },
    {
      "name": "Variable, Property",
      "scope": "variable.property,variable.other.property,variable.other.object.property,variable.object.property,support.variable.property,meta.object-literal.key",
      "settings": {
        "foreground": "#00695C"
      }
    },
    {
      "name": "Markup Headings",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#00796B"
      }
    },
    {
      "name": "Markup Links",
      "scope": "markup.underline.link",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#1565C0"
      }
    },
    {
      "name": "Tag name",
      "scope": "punctuation.definition.tag,entity.name.tag",
      "settings": {
        "foreground": "#00796B"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#00796B"
      }
    },
    {
      "name": "Tag value",
      "scope": "support.constant.property-value,string.quoted.single.tag,string.quoted.double.tag,string.unquoted.tag",
      "settings": {
        "foreground": "#2E7D32"
      }
    },
    {
      "name": "Documentation Block",
      "scope": "comment.block.documentation",
      "settings": {
        "foreground": "#9E9E9E"
      }
    },
    {
      "name": "Documentation Highlight",
      "scope": "keyword.other.documentation",
      "settings": {
        "foreground": "#9E9E9E"
      }
    },
    {
      "name": "Documentation Entity",
      "scope": "entity.name.type.instance.jsdoc",
      "settings": {
        "foreground": "#00796B"
      }
    },
    {
      "name": "Regex",
      "scope": "string.regexp,punctuation.definition.group.regexp,punctuation.definition.character-class.regexp",
      "settings": {
        "foreground": "#AD1457"
      }
    },
    {
      "name": "Annotation, Preprocessor",
      "scope": "meta.preprocessor,entity.name.other.preprocessor,storage.type.annotation,punctuation.definition.annotation",
      "settings": {
        "foreground": "#F57C00"
      }
    },
    {
      "name": "CSS ID, Selector",
      "scope": "meta.selector.css,entity.other.attribute-name.class,entity.other.attribute-name.pseudo-class.css,entity.other.attribute-name.pseudo-element.css",
      "settings": {
        "foreground": "#00796B"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#1565C0"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#F57C00"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#AD1457"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#8E24AA"
      }
    },
    {
      "name": "Ignored (Exceptions Rules)",
      "scope": "punctuation.definition.typeparameters.begin,punctuation.definition.typeparameters.end",
      "settings": {
        "foreground": "#212121"
      }
    }
  ],
  "semanticHighlighting": true
}
